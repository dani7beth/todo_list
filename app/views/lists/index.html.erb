<%= button_to "log Out", destroy_user_session_path, method: :delete%>
<h2>Welcome <%= current_user.first_name%></h2>
<hr />
<%= link_to "Add Tasks", new_list_path %>
<h3>Your Tasks</h3>
<ul>
  <% @lists.each do |list| %>
    <li>
      <%= list.task %>
      <br />
      <%=link_to 'Edit Task', edit_list_path(list.id)%>
      <%=link_to 'Delete Task', list_path(list.id), method: :delete%>
    </li>
    <br />
    <br />
  <% end %>
</ul>
